package elementos;

public class Silueta {
    
	protected String rutaImagen;
	
	public Silueta(String rutaImagen) {
		this.rutaImagen = rutaImagen;
	}
	
	public String getRutaSilueta() {
		return this.rutaImagen;
	}
	
	public void setRutaSilueta(String rutaImagen) {
		this.rutaImagen = rutaImagen;
	}
   
}
