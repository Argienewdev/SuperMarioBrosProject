package elementos.enemigos;

import elementos.entidades.NoJugable;

public abstract class Enemigo extends NoJugable {
	
	protected int puntosPorDestruccion;
    protected int puntosPorMuerte;
    
    public int getPuntosPorDestruccion() {
        return this.puntosPorDestruccion;
    };

    public int getPuntosPorMuerte() {
        return this.puntosPorMuerte;
    }

}
